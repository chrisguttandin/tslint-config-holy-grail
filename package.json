{
    "author": "Christoph Guttandin",
    "bugs": {
        "url": "https://github.com/chrisguttandin/tslint-config-holy-grail/issues"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "dependencies": {
        "@angular/compiler": "^11.2.14",
        "@angular/core": "^11.2.14",
        "codelyzer": "^6.0.2",
        "rxjs": "^6.6.7",
        "rxjs-tslint-rules": "^4.34.8",
        "tslib": "^2.6.1",
        "tslint-etc": "^1.13.10",
        "zone.js": "^0.11.8"
    },
    "description": "This is my personal tslint configuration.",
    "devDependencies": {
        "@commitlint/cli": "^17.7.1",
        "@commitlint/config-angular": "^17.7.0",
        "chai": "^4.3.7",
        "commitizen": "^4.3.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.49.0",
        "eslint-config-holy-grail": "^57.2.20",
        "grunt": "^1.6.1",
        "grunt-cli": "^1.4.3",
        "grunt-sh": "^0.2.1",
        "husky": "^8.0.3",
        "load-grunt-config": "^4.0.1",
        "mocha": "^10.2.0",
        "prettier": "^2.8.8",
        "pretty-quick": "^3.1.3",
        "tslint": "^6.1.3",
        "typescript": "^4.9.5"
    },
    "engines": {
        "node": ">=16.1.0"
    },
    "files": [
        "src/"
    ],
    "homepage": "https://github.com/chrisguttandin/tslint-config-holy-grail",
    "license": "MIT",
    "main": "src/module.js",
    "name": "tslint-config-holy-grail",
    "peerDependencies": {
        "tslint": "^6.1.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/chrisguttandin/tslint-config-holy-grail.git"
    },
    "scripts": {
        "lint": "npm run lint:config && npm run lint:src && npm run lint:test",
        "lint:config": "eslint --config config/eslint/config.json --ext .js --report-unused-disable-directives config/",
        "lint:src": "eslint --config config/eslint/src.json --ext .js --report-unused-disable-directives src/",
        "lint:test": "eslint --config config/eslint/test.json --ext .js --report-unused-disable-directives test/",
        "prepare": "husky install",
        "test": "grunt lint && grunt test"
    },
    "version": "55.0.4"
}
